   "source": [
    "# 15. Digits Sum of a Number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 33,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Write a Python program to calculate the sum of the digits in an integer"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter a number:  67\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "6 + 7 + 13\n"
     ]
    }
   ],
   "source": [
    "num = input(\"Enter a number: \")\n",
    "num2 = num\n",
    "sum = 0\n",
    "for n in range(len(num2)):\n",
    "    sum = int(num[n]) + sum\n",
    "    print(num[n], end=\" + \")\n",
    "    \n",
    "print(sum)"
   ]
