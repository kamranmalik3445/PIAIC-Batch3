    "# 14. Sum of n Positive Integers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "#  Write a python program to sum of the first n positive integers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter a number to find its sum till positive nth number:  7\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Sum of n Positive integers til 7 is 28\n"
     ]
    }
   ],
   "source": [
    "num = int(input(\"Enter a number to find its sum till positive nth number: \"))\n",
    "\n",
    "sum = 0\n",
    "for n in range(1,num+1):\n",
    "    sum = sum + n\n",
    "print(\"Sum of n Positive integers til\", num, \"is\", sum)"
   ]
  },
